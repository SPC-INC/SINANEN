{
  "name": "front-end",
  "description": "フロントエンド開発キット",
  "version": "14.0.000",
  "author": "M-Nakamura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SPC-INC/TaskRunner"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build-time": "npm-run-all -s del time:start build:* time:end",
    "watch": "npm-run-all -s build -p watch:*",
    "clean": "npm-run-all -s del build",
    "restart": "npm-run-all -s del watch",
    "code-format": "prettier --write ./src/**/*.{js,scss} !./src/**/*.min.{js}",
    "style-guide": "npm-run-all -s build -p style-guide:*",
    "del": "node ./_npm-scripts/del.js",
    "build:gitignore": "node ./_npm-scripts/gitignore.js",
    "build:css": "node ./_npm-scripts/css.js",
    "build:php2html": "node ./_npm-scripts/php2html.js --max-old-space-size=4096",
    "build:file": "node ./_npm-scripts/file-util.js --max-old-space-size=4096",
    "build:icon": "node ./_npm-scripts/iconfont.js",
    "build:jsdoc": "jsdoc -r ./src/js -c ./_npm-scripts/jsdoc.js",
    "build:style-guide": "node ./_npm-scripts/style-guide.js",
    "build:stylelint": "node ./_npm-scripts/stylelint.js",
    "build:webpack": "webpack --progress --config ./_npm-scripts/webpack.config.js",
    "watch:browser-sync": "browser-sync start --config ./_npm-scripts/browser-sync.js --max-old-space-size=4096",
    "watch:css": "node ./_npm-scripts/css.js watch",
    "watch:php2html": "node ./_npm-scripts/php2html.js watch --max-old-space-size=4096",
    "watch:jsdoc": "chokidar \"./src/js/*.{js,jsx}\" -c \"jsdoc -r ./src -c ./_npm-scripts/jsdoc.js\" --initial",
    "watch:file": "node ./_npm-scripts/file-util.js watch --max-old-space-size=4096",
    "watch:icon": "node ./_npm-scripts/iconfont.js watch",
    "watch:style-guide": "node ./_npm-scripts/style-guide.js",
    "watch:stylelint": "node ./_npm-scripts/stylelint.js watch",
    "watch:webpack": "webpack --progress --config ./_npm-scripts/webpack.config.js --watch",
    "style-guide:guide": "node ./_npm-scripts/style-guide.js watch",
    "style-guide:css": "npm run watch:css",
    "style-guide:lint": "npm run watch:stylelint",
    "style-guide:icon": "npm run watch:icon",
    "style-guide:browser-sync": "browser-sync start --server --startPath=\"_docs/StyleGuide/\" --files=\"./_docs/StyleGuide/*\"",
    "time:start": "node ./_npm-scripts/time.js",
    "time:end": "node ./_npm-scripts/time.js end"
  },
  "dependencies": {
    "baguettebox.js": "^1.11.1",
    "flatpickr": "^4.6.9",
    "jquery": "^3.6.0",
    "swiper": "^6.7.5",
    "trunk8": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@vusion/webfonts-generator": "^0.7.3",
    "ajv": "^6.12.6",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^7.0.0-beta.3",
    "browser-sync": "^2.27.4",
    "cache-loader": "^4.1.0",
    "caniuse-lite": "^1.0.30001245",
    "chokidar-cli": "^2.1.0",
    "connect-ssi": "^1.1.1",
    "core-js": "^3.15.2",
    "create-react-class": "^15.7.0",
    "cssnano": "^4.1.10",
    "custom-event-polyfill": "^1.0.7",
    "eslint": "^7.30.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "expose-loader": "^1.0.2",
    "frontnote": "^2.0.5",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "9.0.1",
    "imagemin-svgo": "^8.0.0",
    "imagemin-webp": "^6.0.0",
    "jsdoc": "^3.6.7",
    "json-loader": "^0.5.7",
    "node-sass": "^6.0.1",
    "node-sass-css-importer": "0.0.3",
    "npm-run-all": "^4.1.5",
    "php2html": "^4.0.0",
    "postcss": "^8.1.10",
    "postcss-node-sass": "^2.1.8",
    "postcss-scss": "^3.0.4",
    "postcss-sort-media-queries": "^1.31.21",
    "prettier": "^2.3.2",
    "smoothscroll-polyfill": "^0.4.4",
    "stylelint": "^14.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^4.0.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "config": {
    "config": "config.js"
  },
  "browserslist": [
    "> 0.001% in JP and last 2 versions"
  ],
  "keywords": []
}
